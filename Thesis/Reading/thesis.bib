@article{Sharp,
abstract = {14 1. The lowland forest of Borneo is threatened by rapid logging for timber export and clearing for the 15 expansion of timber and oil palm plantations. The combination of these two processes leaves behind 16 landscapes dotted with small, often heavily degraded forest fragments. The biodiversity value of 17 such fragments, which are easily dismissed as worthless, is uncertain. 18 2. We collected 187 taxa of staphylinid beetles across a land-use gradient in Sabah, Malaysia, 19 spanning pristine tropical lowland forest to heavily-degraded forest. Using these data, we identified 20 shifts in alpha-, beta-, and gamma-diversity in response to forest quality and forest area, and applied 21 our findings from continuous expanses of forest to make predictions on hypothetical forest 22 fragments. 23 3. We found that maintaining high forest quality is important for conserving rare taxa (those important 24 for conserving biodiversity per se), and that very small fragments (10 ha) are likely to harbour the 25 same richness of staphylinids as larger fragments (100 ha) of the same forest quality. We estimate a 26 decline in staphylinid richness of just 16{\%} following the removal of 90{\%} of the vegetation biomass 27 from a fragment within this area range. 28 4. Maintaining large forest areas is important for conserving common taxa, likely to be more important 29 for conserving ecosystem functioning. Our analyses suggest that 100 ha fragments of heavily-30 degraded forest can support the same or greater diversity of common taxa as 100 ha fragments of 31 pristine forest. We find that reducing 100 ha fragments to 10 ha fragments will likely result in the loss 32 of just 11{\%} of common taxa diversity. 33 5. Despite significant declines in alpha-diversity, beta-diversity within small rainforest fragments will 34 likely partially mitigate the loss of gamma-diversity, reinforcing the concept that beta-diversity is a 35 dominant force determining the conservation of species in fragmented landscapes. 36 6. Synthesis and applications. In contrast to previous findings on larger animals, our results suggest that even small fragments of degraded forest might be important reservoirs of invertebrate biodiversity in tropical agriculture landscapes. These fragments should be conserved where they occur and form an integral part of management for more sustainable agriculture in tropical landscapes.},
author = {Sharp, Adam C and {L Barclay}, Maxwell V and {C Chung}, Arthur Y and de Rougemont, Guillaume and Turner, Edgar C and Ewers, Robert M},
file = {:home/david/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sharp et al. - Unknown - Forest quality, forest area and the importance of beta-diversity for protecting 1 Borneo's beetle biodiversity.pdf:pdf},
journal = {UNPUBLISHED!},
keywords = {Borneo,Key-words,beetles,beta-diversity,biodiversity,fragmentation,invertebrates,logged forest,spatial},
title = {{Forest quality, forest area and the importance of beta-diversity for protecting 1 Borneo's beetle biodiversity 2 3}}
