@article{Wilkinson,
abstract = {1 Freshwater fish are a vital resource for local communities across the rural tropics. In Southeast Asia, 2 biodiversity-rich forests are being logged and converted to extensive oil-palm monocultures. This 3 clearly impacts associated freshwater ecosystems, but the impact on their biodiversity remains 4 largely understudied and poorly understood, despite the important provisioning service that 5 freshwater fishes provide for human well-being. This study quantifies the biomass stocks of 6 freshwater fish across a land use gradient encompassing primary forest, twice-logged forest and oil 7 palm plantations in Sabah, Malaysia, in an area where local communities are known to harvest 8 freshwater fish. Stream fish were sampled using a cast net, the dominant technique used by local 9 fishermen, in 200 m long transects in 16 streams over three sampling years (2011, 2013 and 2015). 10 Unexpectedly, no impact of land use on total fish availability was detected. There were no significant 11 differences in fish species richness, or most importantly, biomass per unit fishing effort across the 12 land use gradient. There was variation in the responses of five known food species (Tor tambra, 13 Hampala sabana, Barbodes sealei, Barbonymus balleroides and Gastromyzon lepidogaster), and 14 these small differences are attributed to variation in species habitat selection that co-vary with land 15 use change. Despite evidence to suggest freshwater fish communities are resilient to land use 16 change, they still face risks associated with disturbance such as invasion by alien species; 17 furthermore, several of the more stenotopic species were only present in primary forest catchments. 18 Nonetheless, freshwater fish in small headwater streams appear to represent a sustainable food 19 resource for villages established in human-modified forests or developed oil palm plantations. 20 21},
author = {Wilkinson, Clare L and Yeo, Darren C J and Hui, Tan Heok and Fikri, Arman Hadi and Ewers, Robert M},
file = {:home/david/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wilkinson et al. - Unknown - Freshwater fish resource availability is maintained across a land use gradient in Sabah, Borneo.pdf:pdf},
journal = {UNPUBLISHED!},
keywords = {Freshwater fish,Southeast 22 Asia 23 2,deforestation,land use change,oil palm,resource availability},
title = {{Freshwater fish resource availability is maintained across a land use gradient in Sabah, Borneo}}
}
